{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "prepublishOnly": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "artifacts:download": {
      "outputs": [],
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_USERNAME",
    "DATABASE_PASSWORD",
    "DATABASE_HOST",
    "DATABASE_DB_NAME",
    "DATABASE_SSL",
    "PASSPORT_SERVER_URL",
    "NODE_ENV",
    "PORT",
    "HONEYCOMB_API_KEY",
    "MAILGUN_API_KEY",
    "SENDGRID_API_KEY",
    "TIMING",
    "BYPASS_EMAIL_REGISTRATION",
    "ROLLBAR_TOKEN",
    "SW_ID",
    "SUPPRESS_LOGGING",
    "ROLLBAR_ENV_NAME",
    "SERVER_RSA_PRIVATE_KEY_BASE64",
    "DISCORD_TOKEN",
    "DISCORD_ALERTS_CHANNEL_ID",
    "SERVER_EDDSA_PRIVATE_KEY",
    "TELEGRAM_BOT_TOKEN",
    "TELEGRAM_BOT_START_DELAY_MS",
    "TELEGRAM_ANON_WEBSITE",
    "TELEGRAM_ANON_BOT_DIRECT_LINK",
    "MAX_DAILY_ANON_TOPIC_POSTS_PER_USER",
    "TELEGRAM_KUDOSBOT_TOKEN",
    "PASSPORT_CLIENT_URL",
    "PRETIX_SYNC_DISABLED",
    "IS_LOCAL_HTTPS",
    "ACCOUNT_RESET_RATE_LIMIT_DISABLED",
    "ACCOUNT_RESET_LIMIT_DURATION_MS",
    "ACCOUNT_RESET_LIMIT_QUANTITY",
    "WORKER_QUANTITY",
    "ZUCONNECT_TRIPSHA_URL",
    "ZUCONNECT_TRIPSHA_KEY",
    "ZUCONNECT_MOCK_TICKETS",
    "TELEGRAM_ANON_BOT_TOKEN",
    "DEVCONNECT_REDACTION",
    "TELEGRAM_FORWARD_BOT_TOKEN",
    "FROGCRYPTO_ADMIN_USER_EMAILS",
    "FROGCRYPTO_ASSETS_URL",
    "FROGCRYPTO_SERVER_URL",
    "FROG_OWNERS_TELEGRAM_CHAT_ID"
  ]
}
